{
  "core": {
    "databaseVersion": {
      "defaultValue": "000",
      "validations": {
        "isNull": false
      }
    },
    "dbHash": {
      "defaultValue": null
    }
  },
  "admin": {
    "email": {
      "defaultValue": "icollege@example.com",
      "validations": {
        "isNull": false,
        "isEmail": true
      }
    },
    "defaultLang": {
      "defaultValue": "zh_CN",
      "validations": {
        "isNull": false
      }
    }
  },
  "message": {
  },
  "post": {
    "permalinks": {
      "defaultValue": "/:slug/",
      "validations": {
        "matches": "^(\/:?[a-z0-9_-]+){1,5}\/$",
        "matches": "(:id|:slug|:year|:month|:day)",
        "notContains": "/ghost/"
      }
    },
    "description": {
      "defaultValue": "I am iColleger",
      "validations": {
        "isNull": false
      }
    },
    "title": {
      "defaultValue": "iColleger",
      "validations": {
        "isNull": false
      }
    }
  }
}
